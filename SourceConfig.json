		"config_mongo":{
			"url":"mongodb://127.0.0.1:27017",
			"max_open":100,
			"max_idletime":"10m",
			"io_timeout":"500ms",
			"conn_timeout":"500ms"
		}
	},
	"sql":{
		"example_sql":{
			"url":"[username:password@][protocol(address)][/dbname][?param1=value1&...&paramN=valueN]",
			"max_open":100,
			"max_idletime":"10m",
			"io_timeout":"200ms",
			"conn_timeout":"200ms"
		}
	},
	"redis":{
		"example_redis":{
			"url":"[redis/rediss]://[[username:]password@]host[/dbindex]",
			"max_open":100,
			"max_idletime":"10m",
			"io_timeout":"200ms",
			"conn_timeout":"200ms"
		}
	},
